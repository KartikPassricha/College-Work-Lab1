#include <iostream>

using namespace std;
int main()

{
    int x,a;
    cout<<"Enter the size of array"<<endl;
    cin>>x;
    int arr1[x];
    for (int i=0;i<x;i++)
    {
        cin>>arr1[i];
    }
    a=arr1[0];
    for(int i=0;i<x;i++)
    {
        if(a<arr1[i])
        {
            a=arr1[i];
        }
    }
    cout<<"The largest is "<<a<<endl;
    return 0;
}
