#include <iostream>

using namespace std;
int main()

{
    int x,a=0;
    cout<<"Enter the size of array"<<endl;
    cin>>x;
    int arr1[x];
    for (int i=0;i<x-1;i++)
{
        cin>>arr1[i];
    }
    for(int i=0;i<x;i++)
    {
            a=a+arr1[i];
    }
    cout<<"The sum is "<<a<<endl;
    return 0;
}
