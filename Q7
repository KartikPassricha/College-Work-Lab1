#include <iostream>

using namespace std;
int main()

{
    int x,a,c;
    cout<<"Enter the size of array"<<endl;
    cin>>x;
    cout<<"Enter the number to be found"<<endl;
    cin>>a;
    cout<<"Enter the elements of array"<<endl;
    int arr1[x];
    for (int i=0;i<x;i++)
    {
        cin>>arr1[i];
    }
    for(int i=0;i<x;i++)
    {
        if(a==arr1[i])
        {
            cout<<"Number exists";
            return 0;
        }
    }
    cout<<"Number don't exist";
    return 0;


}
